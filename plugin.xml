<?xml version='1.0' encoding='utf-8'?><plugin id="com.mamilist.cordova.taobao.baichuan" version="0.0.1"        xmlns="http://apache.org/cordova/ns/plugins/1.0"        xmlns:android="http://schemas.android.com/apk/res/android">    <name>TaobaoBaiChuanCordova</name>    <description>Cordova plugin for Taobao Baichuan</description>    <license>Apache 2.0</license>    <keywords>cordova,plugin,taobao,baichuan,taobaoke</keywords>    <author>uuleaf@163.com</author>    <engines>        <engine name="cordova" version=">=4.0.0"/>    </engines>    <js-module name="baichuan" src="www/baichuan.js">        <clobbers target="baichuan"/>    </js-module>    <platform name="android">        <source-file                src="src/android/src/mamilist/cordova/baichuan/BaiChuanPlugin.java"                target-dir="src/com/mamilist/cordova/baichuan"/>        <config-file parent="/*" target="res/xml/config.xml">            <feature name="BaiChuan">                <param name="android-package" value="mamilist.cordova.baichuan.BaiChuanPlugin"/>                <param name="onload" value="true"/>            </feature>        </config-file>        <config-file parent="/manifest" target="AndroidManifest.xml">            <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>            <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>            <uses-permission android:name="android.permission.READ_PHONE_STATE"/>            <uses-permission android:name="android.permission.READ_SETTINGS"/>            <uses-permission android:name="android.permission.WRITE_SETTINGS"/>            <uses-permission android:name="android.permission.INTERNET"/>        </config-file>        <config-file target="AndroidManifest.xml" parent="/manifest/application">            <activity android:theme="@android:style/Theme.Black.NoTitleBar"                      android:name="com.alibaba.sdk.android.trade.ui.TradeWebViewActivity"/>            <activity android:theme="@android:style/Theme.Black.NoTitleBar"                      android:name="com.alibaba.sdk.android.trade.ui.NativeTaobaoClientActivity"/>            <activity android:theme="@android:style/Theme.Black.NoTitleBar"                      android:name="com.alibaba.sdk.android.webview.BaseWebViewActivity"/>            <receiver android:exported="false" android:name="com.alibaba.sdk.android.hotpatch.HotPatchBroadcastReceiver"                      android:process=":hotpatch">                <intent-filter>                    <action android:name="com.alibaba.sdk.android.hotpatch.action.HOT_PATCH"/>                </intent-filter>            </receiver>        </config-file>    </platform></plugin>